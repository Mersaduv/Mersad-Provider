    "db:reset": "npx prisma migrate reset --force",
    "db:status": "node check-database-status.js",
    "db:test": "node test-database-connection.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@ckeditor/ckeditor5-build-decoupled-document": "^41.4.2",
    "@ckeditor/ckeditor5-react": "^11.0.0",
    "@vercel/blob": "^0.27.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.542.0",
    "next": "15.5.0",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-leaflet": "^5.0.0",
    "swiper": "^11.2.10",
    "tailwind-merge": "^3.3.1"
